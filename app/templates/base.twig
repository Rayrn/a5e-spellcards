<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{%- block title -%} {{ app_display_name }} | {{ app_version }}{%- endblock -%}</title>
        <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
        <link rel="icon" href="/favicon.ico" type="image/x-icon">

        {% block stylesheets %}
            {{- encore_entry_link_tags('app') -}}
        {%- endblock %}

        {% block javascripts %}
            {{- encore_entry_script_tags('app') -}}
        {%- endblock %}
    </head>
    <body>
        {% include 'nav/nav-main.twig' %}
        {% block body %}{% endblock %}
    </body>
</html>
