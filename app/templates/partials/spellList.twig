{% extends "base.twig" %}

{% block body %}
    <div class="row justify-content-center">
        {% for spell in spellbook.getSpellbookPage(page|default(1), limit).getSpells %}
            {% include 'partials/spell/spellDetail.twig' %}
        {% endfor %}
    </div>

    {% include 'partials/nav/nav-pagination.twig' with {
        'page': page|default(1),
        'limit': limit,
        'total': spellbook.getSpells|length,
    } only %}
{% endblock %}
